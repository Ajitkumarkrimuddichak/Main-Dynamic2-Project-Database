  {{!-- Services section --}}
  <section id="services_section" class="py-5 background2">
    <div class="container">
      <h2 class="text-center text-success">Services that we provide</h2>

      <div class="row mt-5">
        {{!-- first service card --}}
        {{#each services}}
        <div class="col-md-4">
          <div class="card">
            <div class="card-body">
              <div class="container text-center py-3">
                <i class="{{this.icon}} service_icon text-secondary"></i>
              </div>
              <h5 class="card-title">{{this.title}}</h5>
              <p>{{this.description}}</p>
              <a href="{{this.link}}" class="ml-3">Check</a>
              <a href="{{this.linkText}}" class="ml-3">Support</a>
            </div>

          </div>

        </div>
        {{/each}}

      </div>
    </div>
  </section>

